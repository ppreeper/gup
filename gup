	if [ $(id -u) == 0 ]; then
		BDIR="/usr/local/bin"
	else
		BDIR="${HOME}/.local/bin"
	fi

	function download() {
			echo "download $1 version"
			echo "installing ${vers}"
			V=$(echo ${vers} | sed 's/^v//')
			FN=${APP}_extended_${V}_Linux-64bit.tar.gz
			wget -qc ${REPO}/releases/download/${vers}/${FN} -O /tmp/hugo.tar.gz
