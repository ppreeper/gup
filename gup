			FN=${APP}_${vers}_amd64.deb
			rm -f /tmp/${FN}
			wget -qc ${REPO}/releases/download/v${vers}/${FN} -O /tmp/${FN}
			sudo apt install /tmp/${FN}
			rm -f /tmp/${FN}
	}

	download new
}

