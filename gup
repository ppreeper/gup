}

# :command.function
gup_install_oda_command() {

	# src/install_oda_command.sh
	APP="odacli"
	REPO="https://github.com/ppreeper/oda"
	vers=$(git ls-remote --tags ${REPO} | grep "refs/tags.*[0-9]$" | awk '{print $2}' | sed 's/refs\/tags\/v//' | sort -V | uniq | tail -1)

	function download() {
