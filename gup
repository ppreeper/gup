			download new
	else
			APPBIN=$(which ${APP})
			APPVER=$(${APPBIN} version | awk '{print $2}' | awk -F'-' '{print $1}')
			[ "${APPVER}" = "${vers}" ] && echo "${APP} version is current" || download ${vers}
	fi
}

